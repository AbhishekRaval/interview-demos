#
# Dynamically generated by Chef
#
# Local modifications will be overwritten by Chef.
#

# NEVER change the init script itself.
#

JAVA_HOME=<%= @java_home %>

CATALINA_BASE="/opt/tomcat"
CATALINA_HOME="/opt/tomcat"

JAVA_OPTS="<%= @java_options %>"

CATALINA_PID="/opt/tomcat/temp/tomcat.pid"

# JVM parameters passed only for start and run commands
CATALINA_OPTS="<%= @catalina_options %>"

# User-defined Environment variables
<% @environment.each do |envvar| -%>
<%= envvar['VariableName']-%>="<%= envvar['VariableValue']-%>"
<% end -%>
